public class Block{
  private String UUID, firstName, lastName, hash, timeStamp;

  public Block(String UUID, String firstName, String lastName, String hash){
    this.UUID = UUID;
    this.firstName = firstName;
    this.lastName = lastName;
    this.hash = hash;
    this.timeStamp = new SimpleDateFormat("yyyy.MM.dd.HH.mm.ss").format(new Date());
  }
}
